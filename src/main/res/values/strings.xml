    <!-- About -->
    <string name="about_app_title">About Chanobol</string>
    <string name="about_app">
        <![CDATA[
        <p>
        Chanobol is a fast, usable and open-source 4Chan reader inspired by
        Chanu, Clover and more apps.
        </p>

        <p>
        Libraries used include<br/>
        <a href="https://github.com/koush/ion">Ion</a><br/>
        <a href="https://github.com/malmstein/fenster">Fenster</a><br/>
        <a href="https://github.com/chrisbanes/PhotoView">PhotoView</a><br/>
        <a href="http://jsoup.org">jsoup</a><br/>
        <a href="https://github.com/johncarl81/parceler">Parceler</a><br/>
        <a href="http://square.github.io/dagger">Dagger</a><br/>
        <a href="http://jakewharton.github.io/butterknife">Butterknife</a>
        </p>

        <p>
        If you want to show your support for Chanobol consider
        buying <a href="https://play.google.com/store/apps/details?id=truefool.premium">True Fool</a>.
        </p>
        ]]>
    </string>
